{
  "name": "simplewrite",
  "version": "0.0.1",
  "description": "Simple mediachain writer contract",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediachain/aleph.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "web3": "^0.16.0",
    "zeppelin-solidity": "0.0.11"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
